{
  "manifest_version": 2,
  "name": "srcAddon",
  "version": "1.0",
  "description": "Adds a red border to all webpages on srcmake.com",
  "icons": 
        {
        "48": "src48x48.png"
        },
  "content_scripts": 
        [
            {
            "matches": ["<all_urls>"],
            "js": ["srcborder.js"]
            }
        ]
}